{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:vitest-globals/recommended",
    "next/core-web-vitals",
    "next",
    "prettier"
  ],
  "env": {
    "vitest-globals/env": true
  },
  "globals": {
    "React": true
  },
  "rules": {
    "@typescript-eslint/no-unused-vars": "error",
    "no-unused-vars": "off"
  }
}
